    },
    "./plugins/azure-stt": {
      "types": "./dist/index_plugins/AzureSTT_umd.d.ts",
      "es2015": "./dist/esm/plugins/azure-stt-umd-plugin-umd.js",
      "default": "./dist/esm/plugins/azure-stt-umd-plugin-umd.js"
    },
    "./plugins/azure-tts": {
      "types": "./dist/index_plugins/AzureTTS_umd.d.ts",
      "es2015": "./dist/esm/plugins/azure-tts-umd-plugin-umd.js",
      "default": "./dist/esm/plugins/azure-tts-umd-plugin-umd.js"
    },
    "./plugins/aws-st": {
      "types": "./dist/index_plugins/KoreAWSST_umd.d.ts",
      "es2015": "./dist/esm/plugins/koreawspolly-st-umd-plugin-umd.js",
      "default": "./dist/esm/plugins/koreawspolly-st-umd-plugin-umd.js"
    },
    "./plugins/desktop-notification": {
      "types": "./dist/index_plugins/KoreDesktopNotification_umd.d.ts",
      "es2015": "./dist/esm/plugins/kore-desktop-notification-umd.js",
      "default": "./dist/esm/plugins/kore-desktop-notification-umd.js"
    },
    "./plugins/search-suggestions": {
      "types": "./dist/index_plugins/SearchSuggestions_umd.d.ts",
      "es2015": "./dist/esm/plugins/search-suggestions-umd.js",
      "default": "./dist/esm/plugins/search-suggestions-umd.js"
    },
    "./package.json": "./package.json"
  },
  "dependencies": {
    "aws-sdk": "^2.1102.0",
    "d3": "^7.1.1",
    "express": "~4.18.2",
    "jquery": "^3.6.0",
    "jquery-ui": "^1.12.1",
    "jquery.tmpl": "0.0.2",
    "microsoft-cognitiveservices-speech-sdk": "^1.24.1",
    "moment": "^2.24.0",
    "perfect-scrollbar": "^1.5.2",
    "pubsub-js": "^1.9.4",
    "util": "^0.12.4"
  },
  "devDependencies": {
    "@types/jquery": "^3.5.6",
    "@types/pubsub-js": "^1.8.3",
    "@babel/core": "^7.15.5",
    "@babel/plugin-proposal-class-static-block": "^7.16.7",
    "@babel/preset-env": "^7.15.6",
    "@types/aws-sdk": "^2.7.0",
    "@types/node": "^17.0.45",
    "@typescript-eslint/eslint-plugin": "^5.3.1",
    "@typescript-eslint/parser": "^5.3.1",
    "babel-loader": "^8.2.2",
    "css-loader": "^6.3.0",
    "eslint": "^8.11.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-webpack-plugin": "^3.1.1",
    "fs-extra": "^10.1.0",
    "google-charts": "^2.0.0",
    "html-webpack-plugin": "^5.3.2",
    "imports-loader": "^3.0.0",
    "jsdoc": "^3.6.10",
    "postcss": "^8.3.7",
    "postcss-css-variables": "^0.18.0",
    "postcss-loader": "^6.1.1",
    "prettier": "^2.6.0",
    "sass": "^1.42.1",
    "sass-loader": "^12.1.0",
    "style-loader": "^3.3.0",
    "terser-webpack-plugin": "^5.3.1",
    "ts-loader": "^9.2.6",
    "typedoc-plugin-rename-defaults": "^0.6.4",
    "typescript": "^4.4.3",
    "webpack": "^5.54.0",
    "webpack-cli": "~5.0.0",
    "webpack-dev-server": "^4.2.1"
  }
}
